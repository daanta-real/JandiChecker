<?xml version="1.0" encoding="UTF-8"?>
<configuration>

	<!-- 1. Appenders -->
	<!-- STDOUT -->
	<appender name="STDOUT" class="utils.logging.JTextAppender">
		<pattern>
			[%d{yyyy-MM-dd HH:mm:ss}:%-3relative][%thread] %-5level %logger{36} -ì«º- %msg%n
		</pattern>
	</appender>

	<!-- 2. Output level -->
	<root level="TRACE">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>
